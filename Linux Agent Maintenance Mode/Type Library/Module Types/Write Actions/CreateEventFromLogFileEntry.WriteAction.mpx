                  <Param>$Config/Description$</Param>
                </Params>
              </ConditionDetection>
              <WriteAction ID="CollectEvent" TypeID="SC!Microsoft.SystemCenter.CollectEvent"/>
            </MemberModules>
            <Composition>
              <Node ID="CollectEvent">
                <Node ID="Mapper"/>
              </Node>
            </Composition>
